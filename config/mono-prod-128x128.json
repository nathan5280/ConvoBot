{
    "Environment": {
        "Description": "128x128 Mono Image",
        "Date": "Sat Sep 30 08:53:25 MDT 2017",
        "ImageSize": [
            128,
            128
        ],
        "Channels": 3
    },
    "Simulation": {
        "SimulatorName": "mono",
        "CameraHeight": 5,
        "OutputDirName": "images/test-128x128",
        "Radius": {
            "Range": {
                "Min": 15.0,
                "Max": 30.0,
                "Step": 0.4
            }
        },
        "Theta": {
            "Range": {
                "Min": 0.0,
                "Max": 360.0,
                "Step": 1.0
            }
        },
        "Alpha": {
            "Range": {
                "Min": -10.0,
                "Max": 10.0,
                "Step": 2.5,
                "MinFactor": 1,
                "MaxFactor": 1
            }
        }
    },
    "Transformer": {
        "Pipeline": [
            {
              "TransformerName": "color-converter",
              "InputDirName": "images/test-128x128",
              "OutputDirName": "prepared-images/test-128x128"
            },
            {
              "TransformerName": "image-to-nparray",
              "InputDirName": "prepared-images/test-128x128",
              "OutputDirName": "prepared-images/test-128x128"
            }
        ]
    },
    "Model": {
        "Name": "convobot",
        "ModelName": "convobot1",
        "InputDirName": "prepared-images/test-128x128",
        "OutputDirName": "model/test-128x128",
        "ModelFilename": "model.h5",
        "Resume": true,
        "Data": {
            "Label": "prepared-images/test-128x128/label.npy",
            "Image": "prepared-images/test-128x128/image.npy"
        },
        "ResumePhase": 2,
        "Split": true,
        "ValidationSplit": 0.25,
        "TestSplit": 0.1,
        "Schedule": [
            {
                "Name": "phase 1",
                "Sessions": 5,
                "Epochs": 2,
                "BatchSize": 100,
                "LearningRate": 0.005
            },
            {
                "Name": "phase 2",
                "Sessions": 5,
                "Epochs": 2,
                "BatchSize": 100,
                "LearningRate": 0.0025
            },
            {
                "Name": "phase 3",
                "Sessions": 50,
                "Epochs": 10,
                "BatchSize": 100,
                "LearningRate": 0.001
            }
        ],
        "TensorBoardPath": "model/test-128x128/graph"
    }
}
